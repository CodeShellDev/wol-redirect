{
	"hosts": {
		"hypervisor": {
			"ip": "192.168.1.1",
			"mac": "XX:XX:XX:XX:XX:XX",
			"startupTime": 40
		},
		"lxc": {
			"ip": "192.168.1.1",
			"id": "100",
			"startupTime": 10
		},
		"vm": {
			"ip": "192.168.1.1",
			"id": "200",
			"startupTime": 10
		}
	},
	"routes": {
		"hypervisor-lxc": {
			"route": ["hypervisor", "lxc"]
		}
	},
	"records": {
		"*.mydomain.com": "hypervisor-lxc"
	}
}
